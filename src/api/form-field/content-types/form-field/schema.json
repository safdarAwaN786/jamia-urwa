{
  "kind": "collectionType",
  "collectionName": "form_fields",
  "info": {
    "singularName": "form-field",
    "pluralName": "form-fields",
    "displayName": "Form Field"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "label": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "text",
        "email",
        "number",
        "select",
        "file",
        "date"
      ],
      "default": "text"
    },
    "placeholder": {
      "type": "string"
    },
    "required": {
      "type": "boolean",
      "default": false
    },
    "options": {
      "type": "json"
    },
    "form": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::form.form",
      "inversedBy": "fields"
    }
  }
}